class Solution {
public:
    vector<vector<int>> generate(int numRows) {
        vector<vector<int>> ans(numRows);

        for (int i=0; i<numRows; i++) {
            vector<int> tmp(i+1);
            tmp[0] = 1;
            for(int j=1; j<i; j++) {
                tmp[j] = ans[i-1][j-1]+ans[i-1][j];
            }
            tmp[i] = 1;
            ans[i] = tmp;
        }

        return ans;
    }
};
